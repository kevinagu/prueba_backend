<h1>Listing photo_users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photo_users.each do |photo_user| %>
      <tr>
        <td><%= photo_user.user %></td>
        <td><%= photo_user.photo %></td>
        <td><%= link_to 'Show', photo_user %></td>
        <td><%= link_to 'Edit', edit_photo_user_path(photo_user) %></td>
        <td><%= link_to 'Destroy', photo_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo user', new_photo_user_path %>
